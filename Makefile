# EXECUTABLE
MiniMaxOpening: GUI.o main.o
	g++ GUI.o main.o -o MiniMaxOpening
##########################################################################################
# .o FILES
GUI.o: GUI.h GUI.cpp
	g++ -c -g GUI.cpp 

main.o: GUI.h main.cpp
	g++ -c -g main.cpp
##########################################################################################
clean:
	rm *.o MiniMaxOpening
